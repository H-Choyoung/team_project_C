CREATE TABLE gyonggi_db.gyeonggi (
  id INT NOT NULL AUTO_INCREMENT,
  region VARCHAR(100) NOT NULL COMMENT '지역명',
  company_name VARCHAR(100) NOT NULL COMMENT '업체명',
  adress VARCHAR(100) NOT NULL COMMENT '주소',
  call_number VARCHAR(100) NOT NULL COMMENT '전화번호',
  open_time VARCHAR(255) NOT NULL COMMENT '이용시간',
  homepage VARCHAR(255) NOT NULL COMMENT '홈페이지',
  PRIMARY KEY (id)
);

